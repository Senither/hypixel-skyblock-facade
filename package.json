{
  "private": true,
  "name": "hypixel-skyblock-facade",
  "version": "0.11.2",
  "description": "A stateless API facade for the Hypixel SkyBlock API",
  "license": "MIT",
  "scripts": {
    "start": "npm run-script build && node build/index",
    "build": "tsc && npm run-script copy-files",
    "watch": "nodemon src/index.ts",
    "copy-files": "copyfiles -u 1 src/**/*.html build/"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "express": "^4.17.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  }
}
